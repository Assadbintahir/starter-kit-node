        "method-override": "^3.0.0",
        "morgan": "^1.10.0",
        "uuid": "^9.0.1",
        "winston": "^3.11.0",
        "winston-daily-rotate-file": "^5.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.23.9",
        "@babel/preset-env": "^7.23.9",
