        "start:production": "NODE_ENV=production node dist/main.js",
        "dev": "NODE_ENV=development nodemon src/main.ts",
        "lint": "eslint --ignore-path .gitignore --ext .js,.ts",
        "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
        "test": "jest"
