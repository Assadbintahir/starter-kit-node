        "@typescript-eslint/parser": "^7.0.1",
        "babel-jest": "^29.7.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.3",
        "prettier": "^3.2.5",
        "supertest": "^6.3.4",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
